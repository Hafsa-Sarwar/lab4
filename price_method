def get_price(self, quantity):
    try:
        if quantity <= 0:
            raise ValueError("Quantity can't be less than 1.")
        
        if quantity < 10:
            discount = 0
        elif 10 <= quantity < 100:
            discount = 0.10  
        else:
            discount = 0.20 

        total_price = quantity * self.price * (1 - discount)
        return total_price

    except ValueError as e:
        print("Price calculation failed:", e)
        return None  
    # any other exception
    except Exception as e:
        print("Price calculation failed:", e)
        return None
     
